		"tslib": "^1.10.0",
		"typescript": "^3.7.2"
	},
	"peerDependencies": {
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"styled-components": "^4.4.1"
	},
	"dependencies": {
		"@tpr/core": "file:../core",
		"@alekna/react-data-browser": "^1.0.6",
		"lodash": "^4.17.15"
	}
