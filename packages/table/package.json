		"@types/styled-components": "^4.4.2",
		"babel-jest": "^24.9.0",
		"babel-loader": "^8.0.6",
		"codecov": "^3.6.1",
		"cross-env": "^6.0.3",
		"docz": "^2.2.0",
		"jest": "^24.9.0",
		"npm-run-all": "^4.1.5",
		"react": "^16.12.0",
		"react-docgen-typescript-loader": "^3.6.0",
